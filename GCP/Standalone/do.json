{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP standalone 2nic",
    "Common": {
        "class": "Tenant",
        "hostname": "$${local_host}",
        "dbVars": {
            "class": "DbVariables",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "green",
            "ui.advisory.text": "/Common/hostname",
            "config.allow.rfc3927": "enable"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "169.254.169.254",
                "${dns_server}",
                "2001:4860:4860::8844"
            ],
            "search": [
                "${dns_suffix}",
                "google.internal",
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "${ntp_server}",
                "1.pool.ntp.org",
                "2.pool.ntp.org"
            ],
            "timezone": "${timezone}"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
        },
        "external": {
            "class": "VLAN",
            "mtu": 1460,
            "interfaces": [
                {
                    "name": "1.0",
                    "tagged": false
                }
            ],
            "cmpHash": "default",
            "failsafeEnabled": false,
            "failsafeAction": "failover-restart-tm",
            "failsafeTimeout": 90
        },
        "external-self": {
            "class": "SelfIp",
            "address": "$${local_selfip_ext}/32",
            "vlan": "external",
            "allowService": "none",
            "trafficGroup": "traffic-group-local-only"
        },
        "ext_gw_interface": {
            "class": "Route",
            "target": "external",
            "network": "10.1.10.1/32",
            "mtu": 1460
        },
        "ext_rt": {
            "class": "Route",
            "gw": "10.1.10.1",
            "network": "10.1.10.0/24",
            "mtu": 1460
        },
        "default": {
            "class": "Route",
            "gw": "10.1.10.1",
            "network": "default",
            "mtu": 1460
        },
        "internal": {
            "class": "VLAN",
            "mtu": 1460,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ],
            "cmpHash": "default",
            "failsafeEnabled": false,
            "failsafeAction": "failover-restart-tm",
            "failsafeTimeout": 90
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "$${local_selfip_int}/32",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "int_gw_interface": {
            "class": "Route",
            "target": "internal",
            "network": "10.1.20.1/32",
            "mtu": 1460
        },
        "int_rt": {
            "class": "Route",
            "gw": "10.1.20.1",
            "network": "10.1.20.0/24",
            "mtu": 1460
        }
    }
}
